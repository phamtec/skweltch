{
	"pidFile": "tasks.pid",
	"exePath": "./bin/darwin-4.2.1/debug/threading-multi",
	
	// these are all run in the background and remembered.
	"background": [
		{
			"count": 10, // need 10 of these. first arg to the program is the id.
			"exe": "taskwork",
			"config" : {
				"pullFrom": "tcp://localhost:5557",
				"pushTo": "tcp://localhost:5558"
			}
		},
		{
			// only 1 of these.
			"exe": "tasksink",
			"config" : {
				"pullFrom": "tcp://*:5558"
			}
		}
	],
	
	// this is what is used to feed he machine.
	"run": "taskvent",
	"config" : {
		"pushTo": "tcp://*:5557"
		"syncTo": "tcp://*:5558"
	}
	
}
